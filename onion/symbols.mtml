	<head>
		<meta charset="utf-8">
		<title>unicode symbols</title>
	</head>
	<body>
		<header>
			<h1>unicode symbols page for copy-paste</h1>
		</header>
		<h2 id=mathematics>mathematics</h2>
		<table>
			<thead>
				<tr><th scope="col">name</th><th scope="col">symbol</th></tr>
			</thead>
			<tr><td>plus-minus sign</td><td><font size="+2">±</font></td></tr>
			<tr><td>minus-plus sign</td><td><font size="+2">∓</font></td></tr>
			<tr><td>less-than or equal to</td><td><font size="+2">≤</font></td></tr>
			<tr><td>greater-than or equal to</td><td><font size="+2">≥</font></td></tr>
			<tr><td>for all</td><td><font size="+2">∀</font></td></tr>
			<tr><td>because of</td><td><font size="+2">⇐</font></td></tr>
			<tr><td>there exists</td><td><font size="+2">∃</font></td></tr>
			<tr><td>there does not exist</td><td><font size="+2">∄</font></td></tr>
			<tr><td>end of proof</td><td><font size="+2">∎</font></td></tr>
			<tr><td>sigma sum</td><td><font size="+2">∑</font></td></tr>
			<tr><td>sqrt</td><td><font size="+2">√</font></td></tr>
			<tr><td>dot operator</td><td><font size="+2">⋅</font></td></tr>
			<tr><td>much less than</td><td><font size="+2">≪</font></td></tr>
			<tr><td>much greater than</td><td><font size="+2">≫</font></td></tr>
			<tr><td>3 root</td><td><font size="+2">∛</font></td></tr>
			<tr><td>4 root</td><td><font size="+2">∜</font></td></tr>
			<tr><td>proportional to</td><td><font size="+2">∝</font></td></tr>
			<tr><td>identical to || modulo</td><td><font size="+2">≡</font></td></tr>
			<tr><td>infinity</td><td><font size="+2">∞</font></td></tr>
			<tr><td>power 2</td><td><font size="+2">²</font></td></tr>
			<tr><td>power 3</td><td><font size="+2">³</font></td></tr>
			<tr><td>quarter</td><td><font size="+2">¼</font></td></tr>
